  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  <UsingTask TaskName="JSBuild.JSBuildTask" AssemblyFile="..\..\Tools\v3.5\JSBuild.dll" />
  <UsingTask TaskName="AjaxMin" AssemblyFile="..\..\ajaxmintask.dll" />
  <Target Name="BeforeBuild">
    <MakeDir Directories="ScriptOutput" />
    <!-- clean out old files from previous build -->
    <ItemGroup>
      <ToDelete Include="ScriptOutput\**\*" />
    </ItemGroup>
    <Delete Files="@(ToDelete)" />
    <!--
    Delete all the directories. No built-in msbuild task can do this.
    See the following for explaination:
    http://social.msdn.microsoft.com/forums/en-US/msbuild/thread/d2833d1e-2d56-4fc4-98a4-e321b976b9ac
    -->
    <Exec Command="for /f %%d in ('dir /ad /b') do rd /s /q %%d" WorkingDirectory="ScriptOutput" />
    <!-- Build pre.js files that are in the server project -->
    <Message Text="JSBuild: Executing JavaScript Preprocessor." />
    <ItemGroup>
      <PreJS Include="**\*.pre.js">
        <InProject>False</InProject>
      </PreJS>
    </ItemGroup>
    <JSBuildTask SourceFiles="@(PreJS)" SourceDirectory="$(MSBuildProjectDirectory)" DestinationDirectory="ScriptOutput" />
    <!-- minify the release scripts (overwrite them) -->
    <ItemGroup>
      <JS Include="ScriptOutput\**\*.js" Exclude="ScriptOutput\**\*.debug.*" />
    </ItemGroup>
    <AjaxMin JSSourceFiles="@(JS)" JSSourceExtensionPattern="\.js$" JSTargetExtension=".js" JSCollapseToLiteral="true" JSCombineDuplicateLiterals="true" JSLocalRenaming="CrunchAll" JSMacSafariQuirks="true" JSRemoveUnneededCode="true" JSStripDebugStatements="true" JSEvalTreatment="Ignore" JSInlineSafeStrings="true" />
    <!-- Copy Server project Images and CSS into the Scripts Folder -->
    <ItemGroup>
      <StaticResources Include="**\*.png;**\*.jpg;**\*.gif;**\*.css;" />
    </ItemGroup>
    <Copy SourceFiles="@(StaticResources)" DestinationFolder="ScriptOutput\%(RecursiveDir)" />
    <!-- Copy client project script output into the server output folder -->
    <ItemGroup>
      <BaseScriptResources Include="..\..\Client\ScriptOutput\**\*" Exclude="..\..\Client\ScriptOutput\**\embed\*;..\..\Client\ScriptOutput\**\loc\*;..\..\Client\ScriptOutput\extended\**\*" />
      <ExtendedScriptResources Include="..\..\Client\ScriptOutput\extended\**\*" Exclude="..\..\Client\ScriptOutput\extended\**\embed\*;..\..\Client\ScriptOutput\extended\**\loc\*" />
      <BaseEmbedScriptResources Include="..\..\Client\ScriptOutput\**\embed\*" Exclude="..\..\Client\ScriptOutput\extended\**\embed\*" />
      <ExtendedEmbedScriptResources Include="..\..\Client\ScriptOutput\extended\**\embed\*" />
    </ItemGroup>
    <Message Text="======== Copying script resources from client project ========" />
    <Copy SourceFiles="@(BaseScriptResources)" DestinationFolder="ScriptOutput\%(RecursiveDir)" />
    <Copy SourceFiles="@(ExtendedScriptResources)" DestinationFolder="ScriptOutput\%(RecursiveDir)" />
    <!--
    Some scripts have a special version that is meant for embedding into the assembly.
    This is because of the localization feature of ScriptResourceHandler, where each localized
    script is not in the assembly - rather, the script is embedded without the resources, and
    then the resources are built dynamically from an embedded strings resource file.
    The embed versions are all in an 'embed' folder within the folder they were meant to be within,
    so copy each to it's parent directory, overwriting the existing files.
    -->
    <Message Text="======== Copying embed script resources from client project ========" />
    <Copy SourceFiles="@(BaseEmbedScriptResources)" DestinationFiles="@(BaseEmbedScriptResources -> 'ScriptOutput\%(RecursiveDir)..\%(Filename)%(Extension)')" OverwriteReadOnlyFiles="true" />
    <Copy SourceFiles="@(ExtendedEmbedScriptResources)" DestinationFiles="@(ExtendedEmbedScriptResources -> 'ScriptOutput\%(RecursiveDir)..\%(Filename)%(Extension)')" OverwriteReadOnlyFiles="true" />
    <!-- Mark everything under the ScriptOutput folder as embedded resource -->
    <ItemGroup>
      <EmbeddedResource Include="ScriptOutput\**\*" />
    </ItemGroup>
  </Target>
  <UsingTask TaskName="JSBuild.CreateJSManifestResourceName" AssemblyFile="..\..\Tools\v3.5\JSBuild.dll" />
  <Target Name="CreateManifestResourceNames" Condition="'@(EmbeddedResource)' != ''" DependsOnTargets="$(CreateManifestResourceNamesDependsOn)">
    <ItemGroup>
      <_Temporary Remove="@(_Temporary)" />
    </ItemGroup>
    <!-- Create manifest names for ICOs -->
    <CreateJSManifestResourceName BaseDirectory="\" ResourceFiles="@(EmbeddedResource)" RootNamespace="AjaxControlToolkit" Condition="'%(Extension)' == '.ico'">
      <Output TaskParameter="ResourceFilesWithManifestResourceNames" ItemName="_Temporary" />
    </CreateJSManifestResourceName>
    <!-- Create manifest names for culture and non-culture Resx files, and for non-culture Non-Resx resources -->
    <CreateJSManifestResourceName BaseDirectory="ScriptOutput\" ResourceFiles="@(EmbeddedResource)" RootNamespace="" Condition="'%(Extension)' != '.ico' and '%(EmbeddedResource.ManifestResourceName)' == '' and ('%(EmbeddedResource.WithCulture)' == 'false' or '%(EmbeddedResource.Type)' == 'Resx')">
      <Output TaskParameter="ResourceFilesWithManifestResourceNames" ItemName="_Temporary" />
    </CreateJSManifestResourceName>
    <!-- Create manifest names for all culture non-resx resources -->
    <CreateJSManifestResourceName BaseDirectory="ScriptOutput\" ResourceFiles="@(EmbeddedResource)" RootNamespace="" PrependCultureAsDirectory="false" Condition="'%(Extension)' != '.ico' and '%(EmbeddedResource.ManifestResourceName)' == '' and '%(EmbeddedResource.WithCulture)' == 'true' and '%(EmbeddedResource.Type)' == 'Non-Resx'">
      <Output TaskParameter="ResourceFilesWithManifestResourceNames" ItemName="_Temporary" />
    </CreateJSManifestResourceName>
    <ItemGroup>
      <EmbeddedResource Remove="@(EmbeddedResource)" Condition="'%(EmbeddedResource.ManifestResourceName)' == ''" />
      <EmbeddedResource Include="@(_Temporary)" />
      <_Temporary Remove="@(_Temporary)" />
    </ItemGroup>
  </Target>
</Project>